        "@types/md5": "^2.3.5",
        "@types/node": "^22.14.1",
        "@types/seedrandom": "^3.0.8",
        "@typescript-eslint/parser": "^8.30.1",
        "copyfiles": "^2.4.1",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
