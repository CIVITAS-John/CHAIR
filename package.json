        "@langchain/community": "^0.3.40",
        "@langchain/core": "^0.3.44",
        "@langchain/google-genai": "^0.2.3",
        "@langchain/groq": "^0.2.2",
        "@langchain/mistralai": "^0.2.0",
        "@langchain/ollama": "^0.2.0",
        "@langchain/openai": "^0.5.5",
        "cash-dom": "^8.1.5",
        "chalk": "^5.4.1",
        "common-path-prefix": "^3.0.0",
        "d3": "^7.9.0",
        "dotenv": "^16.5.0",
        "driver.js": "^1.3.5",
